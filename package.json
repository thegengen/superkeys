{
  "name": "superkeys",
  "displayName": "SuperKeys",
  "description": "An intuitive and easy to use keymap for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "gengen",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:superKeys.activate"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "superKeys.activate",
        "title": "SuperKeys: Activate"
      }
    ],
    "keybindings": [
      {
        "command": "superKeys.activate",
        "key": "alt-space"
      },
      {
        "command": "superKeys.quickOpen",
        "key": "space",
        "when": "superKeys.active"
      },
      {
        "command": "superKeys.file.new",
        "key": "f n",
        "when": "superKeys.active"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "vscode-uri": "^1.0.0"
  }
}
